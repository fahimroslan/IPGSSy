{
   "background": {
      "page": "template/background.html"
   },
   "browser_action": {
      "default_popup": "template/popup.html"
   },
   "commands": {
      "open_extension": {
         "description": "Recording videos",
         "suggested_key": {
            "default": "Alt+Shift+V"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "js/inboxsdk.js", "js/gmail-search.js" ],
      "matches": [ "https://mail.google.com/*" ]
   }, {
      "js": [ "js/content.js", "js/lib/socket.io.js", "js/modules/server.js", "js/gaTag.js", "js/modules/draw.js", "js/helper.js", "js/main.js", "js/jquery-3.6.0.min.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google-analytics.com https://www.googletagmanager.com; object-src 'self'",
   "description": "The best all-in-one online screen capture, video recording and streaming software that is available for free.",
   "externally_connectable": {
      "ids": [ "*" ],
      "matches": [ "*://*.fluvid.com/*", "*://*.vidooly.com/*", "*://localhost/*", "*://*.atlassian.net/*" ]
   },
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "75",
   "name": "Fluvid - Screen Video Recorder",
   "permissions": [ "\u003Call_urls>", "storage", "tabs", "notifications", "desktopCapture", "tabCapture", "contextMenus", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "8.1.0",
   "web_accessible_resources": [ "template/*", "js/modules/*", "js/lib/*", "css/*", "font-awesome/*", "images/*", "sound/*" ]
}
